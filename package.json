{
  "name": "ldi-bot",
  "version": "1.0.0",
  "main": "app/index.js",
  "repository": "https://github.com/sudoaptgetupgrade/lovensediscordintegration.git",
  "author": "Emma (RileyIO)",
  "contributors": [],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "trash app",
    "dev": "tsc -w",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "start": "node ./app/index.js",
    "start:ps:debug": "@powershell -Command $env:DEBUG='ldi:*'; node ./app/index.js",
    "test": "run-s clean build test:n",
    "test:n": "nyc --silent ava --tap --serial | tnyan",
    "test:v": "nyc --silent ava --verbose --serial",
    "test:verbose": "run-s clean build test:v"
  },
  "ava": {
    "failFast": false,
    "files": [
      "app/**/*.spec.js"
    ],
    "sources": [
      "app/**/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "deep-extend": "^0.6.0",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "joi": "^14.3.1",
    "mongodb": "^3.1.10",
    "nedb": "^1.8.0",
    "restify": "^7.3.0"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/deep-extend": "^0.4.31",
    "@types/joi": "^14.0.1",
    "@types/mongodb": "^3.1.18",
    "@types/nedb": "^1.8.6",
    "@types/node": "^10.12.18",
    "@types/restify": "^7.2.6",
    "@types/restify-errors": "^4.3.3",
    "@types/xregexp": "^3.0.29",
    "ava": "^1.0.1",
    "debug": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "tap-nyan": "^1.1.0",
    "trash-cli": "^1.4.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "xregexp": "^4.2.0"
  }
}
