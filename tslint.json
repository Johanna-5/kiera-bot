{
  "rules": {
    "max-line-length": {
      "options": [
        160
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-console": [
      true,
      "log",
      "error"
    ],
    "no-let": true,
    "quotemark": [true, "single", "backtick"]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}